{
  "expo": {
    "name": "spacely",
    "slug": "spacely",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "softwareKeyboardLayoutMode": "resize"
    },
    "ios": {
      "viewportFit": "cover"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/spacely1.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
